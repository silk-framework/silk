  "scripts": {
    "init": "ecc-link-dotfiles && rm .editorconfig && rm -r .gitlab",
    "test": "npm run jest",
    "test-coverage": "export TEST_REPORT_PATH='./test/reports/' && npm run jest -- --coverage",
    "testBamboo": "npm run test-coverage",
    "test:watch": "npm run jest -- --watch",
    "eslint-check": "eslint --print-config .eslintrc.yml && eslint --print-config .eslintrc.yml | eslint-config-prettier-check",
    "lint": "node_modules/.bin/eslint 'src/HierarchicalMapping/**/*.{js,jsx}' --fix",
    "start": "gulp",
    "build": "gulp build",
    "deploy": "gulp deploy",
    "watch": "cross-env NODE_ENV=development webpack --config webpack.dev.watch.config.js --watch",
    "jest": "node ./test/script.js  --no-cache --detectOpenHandles",
    "webpack-build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "webpack-dev-build": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js",
    "webpack-analyzer": "webpack --config webpack.dev.config.js --profile --json > ./analyzer-stats.json && webpack-bundle-analyzer ./analyzer-stats.json",
    "babel": "cross-env NODE_ENV=production babel --no-babelrc --presets=babel-preset-env,babel-preset-react"
  },
